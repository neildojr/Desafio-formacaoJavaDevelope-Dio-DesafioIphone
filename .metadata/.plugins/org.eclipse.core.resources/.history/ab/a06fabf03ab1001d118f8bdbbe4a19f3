package iphone.apps;

import iphone.apps.sistemaIphone.Iphone;

public class Usuario extends Iphone {
	private void ligar() {
		System.out.println("Ligando Iphone...");
		System.out.println("                                                               ");
		System.out.println("Iphone ligado");
		}
	
	public static void main(String[] args) {
		Iphone iphone = new Iphone();
		
		
		iphone.fazendoChamada();
		iphone.receberLigação();
		System.out.println("                                                               ");
		System.out.println("Recebendo chamada...");
		System.out.println("                                                               ");
		System.out.println("Chamada não atendida...");
		iphone.correioDeVoz();
		System.out.println("                                                               ");
		System.out.println("                                                               ");
		System.out.println("Abrindo Ipod");
		iphone.selecionarMusica();
		System.out.println("Reproduzindo...");
		System.out.println("Mc Poze do Rodo - To Voando Alto");
		iphone.pausarMusica();
		System.out.println("                                                               ");
		System.out.println("Abrindo Safari");
		iphone.adicionarNovaAba();
		iphone.exibirPagina();
		iphone.atualizarPagina();
	}
}
